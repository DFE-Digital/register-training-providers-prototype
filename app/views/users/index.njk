{% extends "layouts/main.njk" %}

{% block content %}

<div class="govuk-grid-row">
  <div class="govuk-grid-column-two-thirds">

    <h1 class="govuk-heading-l">
      Users
    </h1>

    <ul class="govuk-list">
      {% for user in users %}
      <li>
        {{ user.firstname + ' ' + user.lastname }} - {{ user.email }}
      </li>
      {% endfor %}
    </ul>

  </div>
</div>

{% endblock %}
